{
    "name": "morning-train/e-conomic",
    "description": "This is my package e-conomic",
    "keywords": [
        "Morning-Train",
        "e-conomic"
    ],
    "homepage": "https://github.com/morning-train/e-conomic",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon JÃ¸nsson",
            "email": "sj@morningtrain.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "MorningTrain\\Economic\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MorningTrain\\Economic\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
